# The vanity redirect map maps to a 
#
map $toplevel $vanity_redirect {
  studentlink /link/bin/uiscgi_studentlink.pl/uismpl/?ModuleName=menu.pl&NewMenu=Home ;
}

map $backend $backend_hostname {
  include /etc/nginx/hosts.map ;
}

map $microsite $micro_backend {
  include /etc/nginx/sites.map;
}

map $toplevel $top_backend {
  default wordpress ;
  include /etc/nginx/sites.map;
}

